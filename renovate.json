{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "labels": [
    "dependencies"
  ],
  "assigneesFromCodeOwners": true,
  "reviewersFromCodeOwners": true,
  "regexManagers": [
    {
      "fileMatch": [
        "^home/\\.chezmoidata\\.toml$"
      ],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>[a-z-]+?) depName=(?<depName>[^\\s]+?)( extractVersionTemplate=(?<extractVersionTemplate>.*?))?\\s[a-z]+? = \"(?<currentValue>.+?)\"\\s"
      ],
      "extractVersionTemplate": "{{#if extractVersionTemplate}}{{{extractVersionTemplate}}}{{else}}^v(?<version>.*)${{/if}}"
    }
  ]
}
